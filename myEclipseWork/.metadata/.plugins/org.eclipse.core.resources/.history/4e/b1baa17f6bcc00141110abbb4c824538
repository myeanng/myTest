                $('.jue').removeClass('jue_on').css('color', '#999');
                $(this).addClass('jue_on').css('color', '#4f96cf');
                if ($(this).attr('thistype') == '1') {

                    $("input[name='Rate']").prop({ disabled: true }).val('12');
                    $("#UseService").val(1);
                } else {
                    $("input[name='Rate']").prop({ disabled: false }).val(22);
                    $("#UseService").val(0);
                }
                $(".RateformError").remove();
            });

            $('#enjoyfirst').click(function(){
                if($('#PayMonths').length>0) {
                    $('#PayMonths').show();
                    $('#Months').hide();
                    
                }
                interest();

                var rateclass=$('#rate').val();
                $('#mirror').each(function(){
                    $('#mojinwenan').hide();
                    $(this)[0].innerHTML = "AA";
                });
                

            });

            $('#notenjoyfirst').click(function(){
                if($('#PayMonths').length>0) {
                    $('#PayMonths').hide();
                    $('#Months').show();
                }
                interest();
                
                var rateclass=$('#rate').val();
                $('#mojinwenan').show();
                   $('#Rate').val($('#MinRate').val());
                //$('.rate').each(function(){
                //    $(this).removeClass(rateclass).addClass(rateclass);
                //});
                //$('.rate1').each(function(){
                //    $(this)[0].innerHTML = rateclass;
                //});

                $('#mirror').each(function(){
                    $(this)[0].innerHTML = rateclass;
                });
            });

        });

    </script>
    <script type="text/javascript">
        var gateway = new servicestack.ClientGateway('http://' + window.location.host);
        var flag = true;
        $("#IsDraw").change(function() {
            if ($(this).prop('checked')) {
                var str1 = "您发布的是非提现标，借款成功后，您的该笔借款资金无法提现至银行卡，只能在拍拍贷站内进行周转";
                var str2 = $(this);
                customFloat(str1, str2);
                $("#reg_error_box_" + $(this).attr('name')).css('top', Number($("#reg_error_box_" + $(this).attr('name')).css('top').split("p")[0]) - 5 + "px");
            } else {
                $("#reg_error_box_" + $(this).attr('name')).remove();
            }
        });
        $("#publish").click(function() {
            if ($(".jue").length > 0) {
                if ($("#UseService").val() == '') {
                    alert('请选择是否享受增值服务');
                    return;
                }
            }
            //验证form
            if ($("#create_list_form").validationEngine('validate') && flag) {
                flag = false;
                $("#publish").attr("disabled", "true");
                if ($("#ListDescription").attr('edit') == 'true') {
                    var months = $("#Months").val();
                    if($("#PayMonths").length >0 && $("#PayMonths").is(":visible")==true) {
                         months = $("#PayMonths").val();
                    }
                    //判断3个输入域是否被编辑过
                    var data = {
                        Type: 4,
                        Title: $("#Title").val(),
                        ListAmount: $("#ListAmount").val(),
                        BorrowType: $("#BorrowType").val(),
                        Months: months,
                        Rate: $("#Rate").val(),
                        Description: $("#ListDescription").val(),
                        BorrowCredit: 10,
                        UseService: $('#UseService').val() == 1
                    };
                    gateway.postToService(
                        {
                            CreateList: data
                        }, function(e) {
                            $("#publish").removeAttr("disabled");
                            //成功信息
                            $("#message").html(e.ResponseStatus.Message);
                            flag = true;
                        }, function(e) {
                            $("#publish").removeAttr("disabled");
                            //错误信息
                            alert(e.ResponseStatus.Message);
                            flag = true;
                        }
                    );
                } else {
                    //如果没编辑 阻止提交
                    $("#publish").removeAttr("disabled");
                    alert("请编辑借款详情后提交");
                    return false;
                }
            }
            return false;
        });

        function customFloat(str1, str2) {
            //添加一个提示浮动层
            $("#content_nav").append("<div id='reg_error_box_" + str2.attr('name') + "'  class='register_float_box' style=\"display: none;\"><div>" + str1 + "</div></div>");
            $("#reg_error_box_" + str2.attr('name')).css('top', str2.offset().top - 3).css('left', str2.offset().left + str2.width() + 15).fadeIn('slow');
        }
    </script>
    <script type="text/javascript">
        $(document).ready(function() {
            $("#10002").attr('class', 'tabon');
            //当选择一个月安全标时，并且默认选择了一个月安全标
            if($("#Months").val()==1 && "4" == 6) {

                //1个月安全标单笔发表额最多不能超过1W                        
                if($('#CurrentAmounts').val()>10000) {
                    $("#ListAmount").attr("data-validation-engine","validate[required,custom[integer],lendmoney[3000,10000]]");                        
                }
                //当选择一个月安全标 最低利率为7
                var html = " <input type='text' style=\"width: 80px\" name=\"Rate\" onfocus=\"rate_alt()\" onblur=\"rate_alt_close()\" id=\"Rate\" data-validation-engine=\"validate[required,custom[only2float],Rate[7 , 12]]\"/>%";
                $("#rate_nav").html(html);
            }
            $("#Months,#PayMonths").change(function() {
                interest();
                var disabled = "disabled='disabled'";
                if ($('#UseService').val() != 1) {
                    disabled = '';
                }
                if ("4" == 6) {
                    var minrate = 8;
                    if ($("#IsDraw").prop("checked")) {
                        minrate = 8;
                    }
                    //当前用户可使用额度
                    var currentcmount = $('#CurrentAmounts').val();
                    
                    //当选择一个月安全标 最低利率为7
                    if($(this).val()==1) {
                        minrate = 7;
                        //1个月安全标单笔发表额最多不能超过1W                        
                        if(currentcmount>10000) {
                            $("#ListAmount").attr("data-validation-engine","validate[required,custom[integer],lendmoney[3000,10000]]");                        
                        }
                    }
                    else {
                        $("#ListAmount").attr("data-validation-engine","validate[required,custom[integer],lendmoney[3000,"+currentcmount+"]]");
                    }
                    var sixrate = " <input type='text' " + disabled + " style=\"width: 80px\" name=\"Rate\" onfocus=\"rate_alt()\" onblur=\"rate_alt_close()\" id=\"Rate\" data-validation-engine=\"validate[required,custom[only2float],Rate[" + minrate + " , 12]]\"/>%";
                    $("#rate_nav").html(sixrate);
                } else {
                    //判断还款月份切换利率
                    if ($(this).val() >= 4) {
                        var halfyearrate = " <input type='text' " + disabled + " style=\"width: 80px\" name=\"Rate\" onfocus=\"rate_alt()\" onblur=\"rate_alt_close()\" id=\"Rate\" data-validation-engine=\"validate[required,custom[only2float],Rate[22 , 24]]\"/>%";
                        $("#rate_nav").html(halfyearrate);
                    } else if ($(this).val() <= 3) {
                        var yearRatey = " <input type='text'" + disabled + " style=\"width: 80px\" name=\"Rate\" id=\"Rate\" onfocus=\"rate_alt()\" onblur=\"rate_alt_close()\" data-validation-engine=\"validate[required,custom[only2float],Rate[22 ,22.4]]\"/>%";
                            $("#rate_nav").html(yearRatey);
                        }
                }


                if ($("#UseService").val() == "1") {
                    $(".jue").eq(0).click();
                } else {
                    $(".jue").eq(1).click();
                }


            });
            $(".about_list").children('textarea').focus(function() {
                //如果输入域被编辑改编为编辑状态，且去除提示语
                if ($(this).attr('edit') != 'true') {
                    $(this).val('');
                }
                $(this).attr('edit', 'true');
            });
            $("#Title").focus(function() {
                //借款标题提示语
                var str1 = "请认真填写，建议扼要简单说明借款目的，此为借出者支持您的第一印象。";
                var str2 = $(this);
                customFloat(str1, str2);
            });
            $("#Title").focusout(function() {
                //去除借款标题提示语
                $("#reg_error_box_" + $(this).attr('name')).remove();
            });
            //ListDescription
            $("#ListDescription").focus(function() {
                //借款标题提示语
                var str1 = "关于我/我想要使用这笔款项做什么/我的还款能力说明";
                var str2 = $(this);
                customFloat(str1, str2);
                $("#reg_error_box_" + str2.attr('name')).css('top', str2.offset().top + 112).css('left', str2.offset().left).fadeIn('slow');
            });
            $("#ListDescription").focusout(function() {
                //去除借款标题提示语
                $("#reg_error_box_" + $(this).attr('name')).remove();
            });


            $("#ListAmount").focus(function() {
                //借款金额提示语

                var str1 = "您当前的额度为" + 3000 + "元";
                var str2 = $(this);
                customFloat(str1, str2);
            });
            $("#ListAmount").blur(function() {
                //计算利息
                interest();
            });
            $("#ListAmount").focusout(function() {
                //去除借款金额提示语
                $("#reg_error_box_" + $(this).attr('name')).remove();
            });

        });

        function rate_alt() {
            //年利率提示语
            var str1;
            if ($("#Months option:selected").val() >= 4) {
                     str1 = "根据您的信用分您的可发标利率范围在" + 22 + "%-" + 24 + "%之间";
            } else if ($("#Months option:selected").val() <= 3) {
                     str1 = "根据您的信用分您的可发标利率范围在" + 22 + "%-" + 22.4 + "%之间";
            }
            var str2 = $("#Rate");
            customFloat(str1, str2);
        }

        function rate_alt_close() {
            //去除利率提示语
            $("#reg_error_box_" + $("#Rate").attr('name')).remove();
                interest();
        }

        function interest() {
            

          
            var m = $("#Months");
            if($("#PayMonths").length >0 && $("#PayMonths").is(":visible")) {
                m = $("#PayMonths");
            }
            
            var months = parseFloat(m.val());
            var rates = parseFloat($("#Rate").val()) / 12 / 100;
            var amounts = parseFloat($("#ListAmount").val());
            var p = Math.pow(1 + rates, months);
            var payment = Math.round((amounts * rates * p / (p - 1) * months - amounts) * 100) / 100;
            if (isNaN(payment) == false) $("#interestmsg").html(payment);
            
            var Fee = 0;//手续费
             
                   
            Fee=($("#ListAmount").val() * 7);
                   

            if (m.find('option:selected').val() <= 6) {
                $('div[thistype="1"]').find('span').text((($("#ListAmount").val() * 2) / 100 + Fee / 100).toFixed(2));
                $('div[thistype="0"]').find('span').text((($("#ListAmount").val() * 2) / 100).toFixed(2));
            } else if (m.find('option:selected').val() > 6) {
                $('div[thistype="1"]').find('span').text((($("#ListAmount").val() * 4) / 100 + Fee / 100).toFixed(2));
                $('div[thistype="0"]').find('span').text((($("#ListAmount").val() * 4) / 100).toFixed(2));
            }
        }

        $.post("/visitlog", { typeid: 1, remark: '4', checkExist: true });
    </script>


